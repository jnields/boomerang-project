{
  "env": {
    "es6": true
  },
  "extends": "airbnb",
  "globals": {
    "require": true,
    "module": false,
    "exports": false,
    "browser": true,
    "server": true
  },
  "rules": {
    "no-console": "off",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1,
        "outerIIFEBody": 1,
        "FunctionDeclaration": {
          "parameters": 1,
          "body": 1
        },
        "MemberExpression": 1,
        "FunctionExpression": {
          "parameters": 1,
          "body": 1
        },
        "CallExpression": {
          "arguments": 1
        },
        "ArrayExpression": 1,
        "ObjectExpression": 1
      }
    ],
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "max-len":"error",
    "max-lines": "error",
    "import/extensions": [
      "error",
      {
        "css": "never",
        "scss": "never",
        "sass": "never"
        }
      ]
  }
}
