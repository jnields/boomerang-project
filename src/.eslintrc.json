{
  "env": {
    "es6": true
  },
  "extends": "airbnb",
  "globals": {
    "require": true,
    "module": false,
    "exports": false
  },
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/extensions": [
      "error",
      {
        "css": "never",
        "scss": "never",
        "sass": "never"
        }
      ]
  }
}
