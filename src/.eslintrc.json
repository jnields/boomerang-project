{
    "extends": [
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "globals": {
        "require": true,
        "module": false
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 7,
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true,
            "experimentalObjectRestSpread": true
        }
    }
}
