{
  "name": "boomerang-project",
  "version": "1.0.0",
  "description": "Part of the Boomerang Project web app for generation of name tags and database interaction, with excel parsing",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx --fix .",
    "prepare": "NODE_ENV=production npm run build",
    "prebuild": "rimraf ./public",
    "build": "npm run build-scripts && npm run build-client && npm run build-server",
    "build-scripts": "webpack --config webpack.config.scripts.babel.js",
    "build-client": "webpack --config webpack.config.client.babel.js",
    "build-server": "webpack --config webpack.config.server.babel.js",
    "start:dev": "webpack-dev-server --config webpack.config.client.babel.js & webpack --watch --config webpack.config.server.babel.js & nodemon --watch dev-server.js dev-server.js & wait;",
    "test": "exit 1"
  },
  "author": "Joseph Nields",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.2.4",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.64.0",
    "glob": "^7.1.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-node-externals": "^1.6.0",
    "worker-loader": "^0.7.1"
  },
