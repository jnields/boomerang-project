{
  "name": "boomerang-project",
  "version": "1.0.0",
  "description": "Part of the Boomerang Project web app for generation of name tags and database interaction, with excel parsing",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix .",
    "clean": "rm -rf ./public server-bundle.js*",
    "build": "npm run clean && npm run build:prod",
    "build:prod": "npm run build-client:prod && npm run build-server:prod",
    "build-scripts": "webpack --config webpack.config.scripts.js",
    "build-client:dev": "webpack-dev-server --config webpack.config.client.dev.js",
    "build-client:prod": "webpack --config webpack.config.client.prod.js",
    "build-server:dev": "webpack --config webpack.config.server.dev.js --watch",
    "build-server:prod": "webpack --config webpack.config.server.prod.js",
    "start": "if [ $NODE_ENV = 'production' ]; then npm run start:prod; else npm run start:dev; fi;",
    "start:dev": "npm run build-client:dev & npm run build-server:dev & trap 'killall background &> /dev/null' SIGINT SIGHUP SIGTERM; nodemon --watch server-bundle.js server-bundle.js;",
    "start:prod": "npm run build && node server-bundle.js",
    "test": "exit 1",
    "test:watch": "exit 1"
  },
  "author": "Joseph Nields",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.16.0",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.0",
    "debug": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "http": "0.0.0",
    "https": "^1.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.14.2",
    "morgan": "^1.7.0",
    "mysql": "^2.13.0",
    "node-sass": "^4.5.2",
    "nodemailer": "^4.0.0",
    "normalizr": "^3.2.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.8",
    "proxy-middleware": "^0.15.0",
    "pug": "^2.0.0-beta6",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.5.2",
    "redux-form": "^6.6.3",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^6.0.3",
    "sequelize": "^3.30.1",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "worker-loader": "^0.7.1",
    "xhr": "^2.4.0",
    "xlsx": "^0.9.9"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "react",
      "stage-1",
      [
        "env",
        {
          "modules": false,
          "targets": {
            "uglify": true
          },
          "useBuiltIns": true
        }
      ]
    ]
  }
}
